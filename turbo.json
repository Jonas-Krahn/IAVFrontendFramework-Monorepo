{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "release": {
      "dependsOn": ["test", "^test"],
      "cache": false
    },
    "test": {
      "dependsOn": ["build"],
      "cache": false
    },
    "dev": {
      "dependsOn": ["setup"],
      "persistent": true,
      "cache": false
    },
    "build-live": {
      "dependsOn": ["setup"],
      "persistent": true,
      "cache": false
    },
    "build": {
      "dependsOn": ["setup", "^build"],
      "outputs": ["dist/**"]
    },
    "setup": {
      "dependsOn": ["//#setup:root"],
      "cache": false
    },
    "//#setup:root": {
      "cache": false
    }
  }
}
